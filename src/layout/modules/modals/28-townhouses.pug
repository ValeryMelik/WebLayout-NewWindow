#townhouses-modal.modal__window.townhouses
  .townhouses__container
    img(src="../img/modal/townhouses.webp", alt="Таунхаусы").townhouses__mainimg
    .townhouses__content 
      h2.title-reset.townhouses__title Таунхаусы
      p.margin-reset.townhouses__capture г. Омск
      ul.list-reset.townhouses__list 
        li.townhouses__item
          .townhouses__square
          p.townhouses__descr.margin-reset Фасадная система Krauss KRF-35
        li.townhouses__item
          .townhouses__square
          p.townhouses__descr.margin-reset архитектурное стекло Guardian
        li.townhouses__item
          .townhouses__square
          p.townhouses__descr.margin-reset  Площадь остекления&nbsp;&mdash; 1400&nbsp;кв.м.
      img(src="../img/portfolio/pf-liveobj-th-1920.webp" alt="Таунхаусы-2").townhouses__img
      button(type='button').btn-reset.form__closer.closer 
        span.closer__capture Закрыть
        +svg('close', 'closer')